<div class="container">

<div class="col-sm-20">

<h1>Login</h1>
</div>
<br>

<div class="col-sm-20">

<%= form_tag admin_sessions_path do %>
<p>


<%= label_tag :username %> <br>
<%= text_field_tag :username %>

</p>


<p>
<%= label_tag :password %> <br>

<%= text_field_tag :password %>

</p>

<p>
   <%= submit_tag 'Log in' %>
</p>

<% end %>

</div>


</div>
